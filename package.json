    "envalid": "^8.0.0",
    "express": "^5.0.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.4",
    "request-ip": "^3.3.0",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
